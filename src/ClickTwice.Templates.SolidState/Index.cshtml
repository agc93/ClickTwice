	</head>
	<body>

		<!-- Page Wrapper -->
			<div id="page-wrapper">

				<!-- Header -->
					<header id="header" class="alt">
						<h1><a href="index.html">@Model.Manifest.ApplicationName</a></h1>
                        <h2>@Model.Manifest.AuthorName
						<nav>
							<a href="@Model.Launcher">Launch</a>
						</nav>
					</header>

				<!-- Menu -->
					<nav id="menu">
						<div class="inner">
							<h2>Menu</h2>
							<ul class="links">
								<li><a href="@Model.Launcher">Launch</a></li>
								<li><a href="@Model.Installer">Installer</a></li>
								<hr />
								<li><a href="https://github.com/agc93/clicktwice">ClickTwice</a></li>
							</ul>
							<a href="#" class="close">Close</a>
						</div>
					</nav>

				<!-- Banner -->
					<section id="banner">
						<div class="inner">
							<div class="logo"><span class="icon fa-cd"></span></div>
							<h2>@Model.Manifest.ApplicationName</h2>    <h4>@Model.Manifest.AppVersion.ToString()</h4>
							<p>@Model.Manifest.Description</p>
						</div>
					</section>

				<!-- Wrapper -->
					<section id="wrapper">

						<!-- One -->
							<section id="one" class="wrapper spotlight style1">
								<div class="inner">
									<a href="#" class="image"><img src="@Model.ContentDirectory/images/pic01.jpg" alt="" /></a>
									<div class="content">
										<h2 class="major">@Model.Manifest.ShortName</h2>
										<p>@Model.AppInfo.AppInformation</p>
									</div>
								</div>
							</section>

						<!-- Two -->
							<section id="two" class="wrapper alt spotlight style2">
								<div class="inner">
									<a href="#" class="image"><img src="@Model.ContentDirectory/images/pic02.jpg" alt="" /></a>
									<div class="content">
										<h2 class="major">Installation</h2>
										<p>@Model.AppInfo.InstallationInformation</p>
										<a href="@Model.Launcher" class="special">Launch</a>
                                        <a href="@Model.Installer">Install</a>
									</div>
								</div>
							</section>

						<!-- Three -->
							<section id="three" class="wrapper spotlight style3">
								<div class="inner">
									<a href="#" class="image"><img src="@Model.ContentDirectory/images/pic03.jpg" alt="" /></a>
									<div class="content">
										<h2 class="major">Support</h2>
										<p>@Model.AppInfo.SupportInformation</p>
										<a href="@Model.AppInfo.Links.SupportUrl" class="special">Support</a>
                                        <a href="@Model.AppInfo.Links.DocumentationUri" class="special">Documentation</a>
									</div>
								</div>
							</section>
                            
                            <section id="threehalf" class="wrapper spotlight alt style1">
                                <div class="inner">
                                    <div class="content">
                                        <h2 class="major">Requirements</h2>
                                        <h3>Requires .NET Framework @Model.Manifest.FrameworkVersion.ToString()</h3>
                                        @if (Model.Manifest.Prerequisites.Any()) {
                                            <h4>Prerequisites</h4>
                                            <ul class="alt">
                                                @foreach (var prereq in Model.Manifest.Prerequisites) {
                                                    <li>@prereq</li>
                                                }
                                            </ul>
                                        }
                                    </div>
                                </div>
                            </section>

						<!-- Four -->
							<section id="four" class="wrapper spotlight style2">
								<div class="inner">
                                    <div class="content">
									<h2 class="major">Developers</h2>
									<p>@Model.AppInfo.DeveloperInformation</p>
