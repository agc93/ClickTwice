<html>
	<head>
		<title>@Model.Manifest.ApplicationName</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />
		<!--[if lte IE 8]><script src="assets/js/ie/html5shiv.js"></script><![endif]-->
		<link rel="stylesheet" href="@Model.ContentDirectory/assets/css/main.css" />
