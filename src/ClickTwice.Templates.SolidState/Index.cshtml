							<ul class="contact">
								<li class="fa-user">
									@foreach(var author in Model.AppInfo.Author.Names) {
                                        <span>@author</span><br />
                                    }
								</li>
								<li class="fa-phone">@Model.AppInfo.Author.Phone</li>
								<li class="fa-envelope"><a href="#">@Model.AppInfo.Author.Email</a></li>
								<li class="fa-home"><a href="#">@Model.AppInfo.Author.Location</a></li>
							</ul>
							<ul class="copyright">
								<li>&copy; @Model.Manifest.Copyright</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li><li><a href="https://github.com/agc93/clicktwice">ClickTwice</a></li>
							</ul>
						</div>
					</section>

			</div>

		<!-- Scripts -->
			<script src="@Model.ContentDirectory/assets/js/skel.min.js"></script>
			<script src="@Model.ContentDirectory/assets/js/jquery.min.js"></script>
			<script src="@Model.ContentDirectory/assets/js/jquery.scrollex.min.js"></script>
			<script src="@Model.ContentDirectory/assets/js/util.js"></script>
			<!--[if lte IE 8]><script src="assets/js/ie/respond.min.js"></script><![endif]-->
			<script src="@Model.ContentDirectory/assets/js/main.js"></script>

